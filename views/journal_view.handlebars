<div>
    <div class="flipbook-viewport">
        <div class="container">
            <div class="flipbook">
                <div style="background-image:url(/public/pages/ny.jpg)"></div>
                <div style="background-image:url(/public/pages/word1.png)"></div>
                <div style="background-image:url(/public/pages/empire.jpg)"></div>
                <div style="background-image:url(/public/pages/word2.png)"></div>
                <div style="background-image:url(/public/pages/times.jpeg)"></div>
            </div>
        </div>
    </div>
</div>

<script type="text/javascript">

    function loadApp() {

        // Create the flipbook

        $('.flipbook').turn({
            display: 'single',

            // Width

            width:922/2,

            // Height

            height:600,

            // Elevation

            elevation: 50,

            // Enable gradients

            gradients: true,

            // Auto center this flipbook

            autoCenter: true

        });
    }

    // Load the HTML4 version if there's not CSS transform

    yepnope({
        test : Modernizr.csstransforms,
        yep: ['../../lib/turn.js'],
        nope: ['../../lib/turn.html4.min.js'],
        both: ['css/basic.css'],
        complete: loadApp
    });

</script>
</body>
